{
  "name": "@deepgram/deepdown",
  "version": "0.3.1",
  "description": "A markdown templating format for generating AI-ready documentation from JSON Schema, OpenAPI, and AsyncAPI specifications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "lint": "eslint \"**/*.{js,ts,mjs}\"",
    "dev": "tsc -w"
  },
  "keywords": [
    "markdown",
    "template",
    "documentation",
    "openapi",
    "asyncapi",
    "json-schema"
  ],
  "author": "Deepgram",
  "license": "MIT",
  "dependencies": {
    "@deepgram/deepdown-core": "workspace:^0.2.0",
    "@deepgram/deepdown-parser": "workspace:^0.2.0",
    "@deepgram/deepdown-renderer": "workspace:^0.2.1",
    "@deepgram/deepdown-resolver-asyncapi": "workspace:^0.2.1",
    "@deepgram/deepdown-resolver-openapi": "workspace:^0.2.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
